[project]
	description = Docker file for deploying MidoNet
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group midonet-admins
[access "refs/for/refs/*"]
	read = group user/Midokura Bot (midosaba)
	push = group user/Midokura Bot (midosaba)
	label-Code-Review = -2..+2 group user/Midokura Bot (midosaba)
	label-Verified = -1..+1 group user/Midokura Bot (midosaba)
	rebase = group user/Midokura Bot (midosaba)
	removeReviewer = group user/Midokura Bot (midosaba)
	submit = group user/Midokura Bot (midosaba)
[access "refs/heads/*"]
	read = group Registered Users
	read = group midonet-installers
	create = group user/Midokura Bot (midosaba)
	push = +force group midonet-installers
	pushMerge = group midonet-installers
	pushMerge = group user/Midokura Bot (midosaba)
	forgeCommitter = group midonet-installers
	label-Code-Review = -2..+2 group midonet-installers
	label-Code-Review = -1..-1 group Registered Users
	label-Verified = -1..+1 group midonet-installers
	labelAs-Verified = -1..+1 group midonet-installers
	removeReviewer = group midonet-installers
	submit = group midonet-installers
[access "refs/tags/*"]
	push = +force group midonet-installers
	push = +force group user/Midokura Bot (midosaba)
	pushTag = +force group midonet-installers
	pushTag = group user/Midokura Bot (midosaba)
	pushSignedTag = group midonet-installers
	pushSignedTag = group user/Midokura Bot (midosaba)
[access "refs/drafts/*"]
	read = group Registered Users
	push = group Registered Users
[access "refs/publish/*"]
	read = group Registered Users
	push = group Registered Users
